<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="com.apatar.output" version="0.0.1">
	<requires>
		<import plugin-id="com.apatar.core"/>
		<import plugin-id="com.apatar.ui"/>
	</requires>
	<runtime>
	    <library id="output" path="bin/" type="code">
	        <export prefix="*"/>
	    </library>

	</runtime>
	<extension plugin-id="com.apatar.core" point-id="Node" id="OutputNode">
		<parameter id="class" value="com.apatar.output.OutputNodeFactory"/>
	</extension> 	
	
</plugin>
