<?xml version="1.0" encoding="UTF-8"?>

<infinispan
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:infinispan:config:7.0 http://www.infinispan.org/schemas/infinispan-config-7.0.xsd"
        xmlns="urn:infinispan:config:7.0">


   <jgroups>
      <stack-file name="tcp" path="conf/jgroups-tcp.xml"/>
   </jgroups>
   <threads>
      <thread-factory name="infinispan-factory" group-name="infinispan"
                      thread-name-pattern="%G %i" priority="5"/>
      <cached-thread-pool name="infinispan-cached" thread-factory="infinispan-factory"/>
      <blocking-bounded-queue-thread-pool name="infinispan-listener"
                                          thread-factory="infinispan-factory"
                                          core-threads="1" max-threads="1" queue-length="0"
                                          keepalive-time="0"/>
      <blocking-bounded-queue-thread-pool name="infinispan-transport"
                                          thread-factory="infinispan-factory"
                                          core-threads="5" max-threads="10" queue-length="100"
                                          keepalive-time="10000"/>
      <scheduled-thread-pool name="infinispan-eviction" thread-factory="infinispan-factory"/>
      <scheduled-thread-pool name="infinispan-repl-queue" thread-factory="infinispan-factory"/>
   </threads>
   <cache-container name="defaultContainer" statistics="false" default-cache="defaultCache"
                    start="true">
      <transport stack="tcp"/>
      <serialization version="70"/>
      <jmx domain="eu.leads.processor" duplicate-domains="true"></jmx>
      <distributed-cache name="default" mode="SYNC" owners="2" l1-lifespan="600000" start="true"
                         statistics="true">
         <compatibility></compatibility>
          <indexing index="NONE">
              <property name="default.directory_provider">ram</property>
          </indexing>
            <eviction max-entries="1000" strategy="LIRS"/>
            <transaction mode="NONE"/>
            <persistence passivation="false">
                <file-store path="${java.io.tmpdir}" shared="true" preload="false" />
            </persistence>
           <state-transfer enabled="false" await-initial-transfer="false"/>

      </distributed-cache>

       <distributed-cache name="default.webpages" mode="SYNC" owners="2" l1-lifespan="600000" start="true"
                          statistics="true">
           <compatibility></compatibility>
           <indexing index="NONE">
               <property name="default.directory_provider">ram</property>
           </indexing>
           <eviction max-entries="1000" strategy="LIRS"/>
           <transaction mode="NONE"/>
           <persistence passivation="false">
               <file-store path="${java.io.tmpdir}" shared="true" preload="false" />
           </persistence>
           <state-transfer enabled="false" await-initial-transfer="false"/>
       </distributed-cache>

       <distributed-cache name="default.entities" mode="SYNC" owners="2" l1-lifespan="600000" start="true"
                          statistics="true">
           <compatibility></compatibility>
           <indexing index="NONE">
               <property name="default.directory_provider">ram</property>
           </indexing>
           <eviction max-entries="1000" strategy="LIRS"/>
           <transaction mode="NONE"/>
           <persistence passivation="false">
               <file-store path="${java.io.tmpdir}" shared="true" preload="false" />
           </persistence>
           <state-transfer enabled="false" await-initial-transfer="false"/>
       </distributed-cache>


      <distributed-cache name="testCache" mode="SYNC" owners="2" l1-lifespan="600000" start="true"
                         statistics="true">
          <compatibility></compatibility>
          <indexing index="NONE">
              <property name="default.directory_provider">ram</property>
          </indexing>
          <eviction max-entries="1" strategy="LIRS"/>
          <transaction mode="NONE"/>
          <persistence passivation="false">
              <file-store path="${java.io.tmpdir}" shared="true" preload="false" />
          </persistence>
          <state-transfer enabled="false" await-initial-transfer="false"/>
      </distributed-cache>

      <distributed-cache name="clustered" mode="SYNC" owners="2" l1-lifespan="600000" start="true"
                         statistics="true">
          <compatibility></compatibility>
          <indexing index="NONE">
              <property name="default.directory_provider">ram</property>
          </indexing>
          <eviction max-entries="1" strategy="LIRS"/>
          <transaction mode="NONE"/>
          <persistence passivation="false">
              <file-store path="${java.io.tmpdir}" shared="true" preload="false" />
          </persistence>
          <state-transfer enabled="false" await-initial-transfer="false"/>
      </distributed-cache>
      <distributed-cache name="active_plugins_cache" mode="ASYNC" owners="2" l1-lifespan="600000"
                         start="true"
                         statistics="false">
          <compatibility></compatibility>
          <!--<indexing index="NONE">-->
              <!--<property name="default.directory_provider">ram</property>-->
          <!--</indexing>-->
          <eviction max-entries="1" strategy="LIRS"/>
          <transaction mode="NONE"/>
          <persistence passivation="false">
              <file-store path="${java.io.tmpdir}" shared="true" preload="false" />
          </persistence>
          <state-transfer enabled="false" await-initial-transfer="false"/>
      </distributed-cache>
      <distributed-cache name="processor_plugins" mode="ASYNC" owners="2" l1-lifespan="600000"
                         start="true"
                         statistics="false">
          <compatibility></compatibility>
          <!--<indexing index="NONE">-->
              <!--<property name="default.directory_provider">ram</property>-->
          <!--</indexing>-->
          <eviction max-entries="1" strategy="LIRS"/>
          <transaction mode="NONE"/>
          <persistence passivation="false">
              <file-store path="${java.io.tmpdir}" shared="true" preload="false" />
          </persistence>
          <state-transfer enabled="false" await-initial-transfer="false"/>
      </distributed-cache>

      <distributed-cache name="queries" mode="SYNC" owners="2" l1-lifespan="600000" start="true"
                         statistics="true">
          <compatibility></compatibility>
          <!--<indexing index="NONE">-->
              <!--<property name="default.directory_provider">ram</property>-->
          <!--</indexing>-->
          <eviction max-entries="100" strategy="LIRS"/>
          <transaction mode="NONE"/>
          <persistence passivation="false">
              <file-store path="${java.io.tmpdir}" shared="true" preload="false" />
          </persistence>
          <state-transfer enabled="false" await-initial-transfer="false"/>
     </distributed-cache>
     <distributed-cache name="tables" mode="SYNC" owners="2" l1-lifespan="600000" start="true"
                         statistics="true">
         <compatibility></compatibility>
         <!--<indexing index="NONE">-->
             <!--<property name="default.directory_provider">ram</property>-->
         <!--</indexing>-->
         <eviction max-entries="1" strategy="LIRS"/>
         <transaction mode="NONE"/>
         <persistence passivation="false">
             <file-store path="${java.io.tmpdir}" shared="true" preload="false" />
         </persistence>
         <state-transfer enabled="false" await-initial-transfer="false"/>
      </distributed-cache>
   </cache-container>

</infinispan>
